{
  "name": "Checked",
  "description": "You will never miss checkin and checkout anymore",
  "version": "0.0.1",
  "icons": {
    "16": "img/checked.png"
  },
  "action": {
    "default_title": "Let me check it 4u",
    "default_icon": "img/checked.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/jquery-3.6.0.js",
        "js/index.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "manifest_version": 3
}
